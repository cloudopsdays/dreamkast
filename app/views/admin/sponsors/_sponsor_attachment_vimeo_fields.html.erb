<div class="vimeo-field">
  <div class="form-row form-group">
    <div class="col-12 col-mb-6">
      <%= f.label :url, "Vimeo embed url" %>
      <%= f.text_field :url, class: "form-control", id: "attachmentTextInputForm", rows: "3" %>
      <div class="movie-wrap">
        <iframe src="<%= f.object&.url %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
      </div>
    </div>

    <div class="col-12 col-mb-6">
      <%= f.label :text, "Video Description" %>
      <%= f.text_area :text, class: "form-control", id: "attachmentTextInputForm", rows: "3" %>
    </div>
    <%= f.hidden_field :_destroy %>
    <%= link_to 'Delete', '#', class: 'remove_vimeo_field' %>
    <br>
  </div>
  <hr>
</div>
